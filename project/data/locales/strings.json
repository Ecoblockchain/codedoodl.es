        "ABOUT_PAGE": {
            "title": "About page",
            "strings": {
                "about_label_what": "What is this",
                "about_content_what": "codedoodl.es is a showcase of curated creative coding sketches. The aim of these doodles is to showcase interactive, engaging web experiments which only require a short attention span. No loading bars, no GUI, no 5MB 3D models or audio files, just plain and simple doodles with code. The site at <a href=\"http://codedoodl.es\">codedoodl.es</a> will always feature the latest doodles, but we also have a <a href=\"http://chromestore.com\" target=\"_blank\">chrome extension</a> which features a new doodle in every browser tab you open.",
                "about_label_contact": "Find us on",
                "about_content_contact": "<a href=\"http://twitter.com/codedoodl_es\" target=\"_blank\">Twitter</a>, <a href=\"http://github.com/neilcarpenter/codedoodl.es\" target=\"_blank\">Github</a>",
                "about_label_who": "Made by"
            }
        },
        "CONTRIBUTE_PAGE": {
            "title": "Contribute page",
            "strings": {
                "contribute_label_submit": "Submit",
                "contribute_content_submit": "This is a 100% community-based project. All doodle submission and selection is done via github - to contribute, please follow the steps outlined in the <a href=\"http://github.com/neilcarpenter/codedoodl.es\" target=\"_blank\">core site readme</a>. Before submitting make sure you've read our guidelines on <a href=\"http://github.com/neilcarpenter/codedoodl.es\" target=\"_blank\">creative</a> and <a href=\"http://github.com/neilcarpenter/codedoodl.es\" target=\"_blank\">technical</a> criteria. Please note that this is a curated collection, so unfortunately not all submitted doodles can be featured.",
                "contribute_label_contact": "Contact us",
                "contribute_content_contact": "Get us on <a href=\"http://twitter.com/codedoodl_es\" target=\"_blank\">twitter</a>"
            }
        }
